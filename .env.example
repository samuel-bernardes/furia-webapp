# .env.example - Frontend (furia-webapp)

# URL base da API do backend
VITE_API_URL=http://localhost:3333

# Discord OAuth para LOGIN
VITE_DISCORD_CLIENT_ID=your_discord_client_id
VITE_DISCORD_REDIRECT_URL="https://discord.com/oauth2/authorize?client_id=your_discord_client_id&response_type=code&redirect_uri=http%3A%2F%2Flocalhost%3A3333%2Fauth%2Fdiscord%2Fredirect&scope=identify+openid+email"

# Discord OAuth para CONEXÃO (conta já autenticada)
VITE_DISCORD_CONNECTION_URL="https://discord.com/oauth2/authorize?client_id=your_discord_client_id&response_type=code&redirect_uri=http%3A%2F%2Flocalhost%3A3333%2Fconnection%2Fdiscord&scope=identify+email+openid"

# Twitch OAuth
VITE_TWITCH_CLIENT_ID=your_twitch_client_id
